{
    "name": "xyqweb/yii2-queue",
    "description": "Yii2 Queue Extension which supported RabbitMQ",
    "type": "yii2-extension",
    "keywords": ["yii", "queue", "gii", "rabbitmq"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Roman Zhuravlev",
            "email": "zhuravljov@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/yiisoft/yii2-queue/issues",
        "source": "https://github.com/yiisoft/yii2-queue",
        "docs": "https://github.com/yiisoft/yii2-queue/blob/master/docs/guide"
    },
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "~2.0.14",
        "symfony/process": "*",
        "php-amqplib/php-amqplib": "*",
        "xyqweb/amqp-lib": "^0.1.0"
    },
    "require-dev": {
    },
    "suggest": {
        "ext-pcntl": "Need for process signals."
    },
    "autoload": {
        "psr-4": {
            "yii\\queue\\": "src",
            "yii\\queue\\amqp_interop\\": "src/drivers/amqp_interop"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
